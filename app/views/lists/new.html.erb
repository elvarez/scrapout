
<% provide(:title, "Sign in") %>
<h1>New List</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@list) do |f| %>

      <%= f.label :title %>
      <%= f.text_field :title %>
      <%= f.submit "Create list", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>

